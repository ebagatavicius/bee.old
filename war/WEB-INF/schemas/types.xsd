    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="notEmptyString">
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="intOrBoolean">
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="xs:int" />
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:boolean" />
