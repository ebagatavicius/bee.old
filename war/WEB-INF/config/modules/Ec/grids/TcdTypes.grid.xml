    <DataColumn name="TypeName" visible="true" autoFit="true" />

    <DataColumn name="ProducedFrom" visible="true" />
    <DataColumn name="ProducedTo" visible="true" />
    <DataColumn name="Ccm" />
    <DataColumn name="KwFrom" visible="true" />

    <DataColumn name="KwTo" visible="false" />
    <DataColumn name="Cylinders" visible="false" />
    <DataColumn name="MaxWeight" visible="false" />

    <DataColumn name="Engine" visible="false" caption="=ecCarEngine"/>
    <DataColumn name="Fuel" visible="false" caption="=vehicleFuel"/>
    <DataColumn name="Body" visible="false" caption="=vehicleBodyType"/>
    <DataColumn name="Axle" visible="false" caption="=vehicleChassis"/>

    <DataColumn name="Visible" />
