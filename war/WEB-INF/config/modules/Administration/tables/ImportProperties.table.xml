<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="ImportProperties"
  idName="PropertyID"
  >
  <Fields>
    <Relation name="Option" notNull="true" relation="ImportOptions" cascade="DELETE" label="=trImportOption" />
    <String name="Property" precision="30" notNull="true" label="=trImportProperty" />
    <String name="Value" precision="30" label="=trImportValue" />
    <Relation name="RelationOption" unique="true" relation="ImportOptions" cascade="DELETE" editable="true" />
  </Fields>
  <Constraints>
    <Check>
      <PostgreSql>"RelationOption" IS NULL OR "RelationOption" != "Option"</PostgreSql>
      <MsSql />
      <Oracle />
    </Check>
    <Unique fields="Option Property" />
  </Constraints>
</Table>
