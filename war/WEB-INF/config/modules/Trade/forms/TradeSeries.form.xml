<?xml version="1.0" encoding="UTF-8"?>
<f:Form xmlns="http://www.butent.com/widget"
  xmlns:f="http://www.butent.com/form"
  
  name="TradeSeries" viewName="TradeSeries" caption="=trdSeries"
  width="500" height="300"
  >
  <FlowPanel class="flexBox-vertical">
    <br/>
    <Table>
      <col index="0" kind="label" />

      <row>
        <Label for="SeriesName" />
        <InputText source="SeriesName" upperCase="true" style="width: 100px;" visible="true" />
      </row>
      <row>
        <Label for="Notes" />
        <InputArea source="Notes" visibleLines="5" style="width: 350px;" />
      </row>
    </Table>

    <br/>
    <ChildGrid class="flexible" name="SeriesManagers" relColumn="Series" />
  </FlowPanel>
</f:Form>  
