<?xml version="1.0" encoding="UTF-8"?>
<g:Grid xmlns="http://www.butent.com/column"
  xmlns:g="http://www.butent.com/grid"

  name="Attendees" viewName="Attendees"
  caption="=calAttendees" newRowCaption="=calNewAttendee"
  editForm="Attendee" newRowForm="Attendee"
  >
  <g:columns>
    <DataColumn name="Ordinal" editInPlace="true" />

    <DataColumn name="TypeName" label="=calAttendeeType" visible="true" />

    <DataColumn name="Name" minWidth="100" visible="true" />
    <DataColumn name="Description" visible="false" />
    <DataColumn name="Location" visible="false" />

    <DataColumn name="CompanyPerson" renderColumns="FirstName LastName" />

    <DataColumn name="Transparency" />
    
    <DataColumn name="ColorName" label="=color" />

    <DataColumn name="StyleName" label="=style" visible="false" />
    
  </g:columns>
</g:Grid>
